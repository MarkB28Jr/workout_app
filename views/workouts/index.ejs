<%- include('../partials/header') %>
<div id="workoutHead">
    <tr>
      <th>Muscle Group</th>&nbsp;|&nbsp;
      <th>Exercise</th>&nbsp;|&nbsp;
      <th>Sets</th>&nbsp;|&nbsp;
      <th>Reps</th>&nbsp;|&nbsp;
      <th>Weight</th>
    </tr>
</div>
<div id="history">


<% workouts.forEach(function(f) { %>
  <tr>
    <td>
      <%= f.exercise %>
    </td>
    
    <% workout.exercise.forEach(function(e) { %>
    <td>
      <%= e.sets %>
    </td>
    <td>
      <%= e.reps %>
    </td>
    <td>
      <%= e.weight %>
    </td>
    <% }) %>
  </tr>
  <% }); %>



